{
  "name": "vocab-game-web",
  "version": "1.0.0",
  "description": "Interactive vocabulary learning game for TOEFL and IELTS preparation",
  "type": "module",
  "scripts": {
    "start": "npx http-server -p 8080 -c-1",
    "dev": "npx live-server --port=8080",
    "test": "jest",
